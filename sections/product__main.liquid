<!-- sections/product__main.liquid -->
{% comment %} 
  Loop through different blocks to create a product page.

  Globals:
    - settings.layout_horizontal: horizontaly margin
    - settings.thumbmails_color_scheme: set color of items

  Possible enhancements:
    - Add alternative layouts
    - Add support so upsell can fit within form

  Pro tips:
    - Multiple product templates can be created from the theme editor.
{% endcomment %}

{% assign is_product_template = true %}

{% capture script_product %}
  {% render 'script__product', 
    gallery_style: section.settings.media_gallery_style, 
    is_product_template: is_product_template, 
    featured: false, 
    enable_default_selling_plan_widget: section.settings.enable_default_selling_plan_widget
  %}
{% endcapture %}

{% case section.settings.layout_gallery_size %}
  {% when 'w-full md:w-1/3' %}
    {% assign layout_detail_size = 'w-full md:w-2/3' %}
  {% when 'w-full md:w-1/2' %}
    {% assign layout_detail_size = 'w-full md:w-1/2' %}
  {% when 'w-full md:w-2/3' %}
    {% assign layout_detail_size = 'w-full md:w-1/3' %}
  {% when 'w-full md:w-3/4' %}
    {% assign layout_detail_size = 'w-full md:w-1/4' %}
{% endcase %}

<section class="js-product z-10 relative
  {{ section.settings.color_scheme }}"
  style="
    padding-top:{{ section.settings.spacing_top }}px;
    padding-bottom:{{ section.settings.spacing_bottom }}px;"
  x-data="{{ script_product }}"
  x-init="
    attachStandardForm();
    initProductForm();
  "
  x-ref="formContainer"
  @keyup.left="galleryScrollBack();"
  @keyup.right="galleryScrollNext();">

  {% capture form_class %}
    !px-0 md:!px-4 flex flex-row flex-wrap items-start
    {{ settings.layout_horizontal }}
  {% endcapture %}
  {% form "product", product,
    class: form_class,
    x-ref: 'productForm'
  %}

    {% comment %} Media - left column {% endcomment %}
    <div 
      class="
        top-0 md:sticky flex flex-col h-fit
        {{ section.settings.layout_gallery_size }}
      ">

      {% comment %} Media {% endcomment %}
      {% if section.settings.media_gallery_style == 'grid' %}
        {% render 'product__gallery-grid' %}
      {% elsif section.settings.media_gallery_style == 'slider' %}
        {% render 'product__gallery-slider' %}
      {% else %}
        {% render 'product__gallery-thumbnail-slider' %} 
      {% endif %}

      {% comment %} Blocks - desktop only {% endcomment %}
      <div 
        class="hidden md:block">
        {% for block in section.blocks %}
          {% if block.settings.position == "left" %}
            {% case block.type %}
                
              {% when 'breadcrumb' %}
                {% render 'product__breadcrumb', blocksettings: block.settings %}
              
              {% when 'title' %}
                {% render 'product__title', blocksettings: block.settings %}

              {% when 'price' %}
                <div class="px-4
                    {{ block.settings.color_border }} 
                    {{ block.settings.color_scheme }} 
                    {{ block.settings.style_border }}"
                  style="
                    padding-top:{{ block.settings.spacing_top }}px;
                    padding-bottom:{{ block.settings.spacing_bottom }}px;">
                  <div class="relative flex items-end justify-between">
                    {% render 'product__price', blocksettings: block.settings %}
                  </div>
                  <div class="mt-1 type--small">
                    {{ form | payment_terms }}
                  </div>
                </div>

              {% when 'custom' %}
                <div class='px-4
                  {{ block.settings.color_border }} 
                  {{ block.settings.color_scheme }} 
                  {{ block.settings.style_border }}'
                  style="
                    padding-top:{{ block.settings.spacing_top }}px;
                    padding-bottom:{{ block.settings.spacing_bottom }}px;">
                  {% render 'product__custom-field', blocksettings: block.settings %}
                </div>
              
              {% when 'text' %}
                {% render 'product__text', blocksettings: block.settings %}

              {% when 'description' %}
                <div>
                  {% render 'product__description', blocksettings: block.settings %}
                </div>

              {% when 'accordion' %}
                {% render 'product__accordion', blocksettings: block.settings %}

              {% when 'popup' %}
                {% render 'product__popup', blocksettings: block.settings %}

              {% when 'inventory' %}
                {% render 'product__inventory', blocksettings: block.settings %}
                
              {% when 'pickup' %}
                {% render 'product__pickup', blocksettings: block.settings %}    
                
              {% when 'options' %}
                <div class="options
                  {{ block.settings.color_border }}
                  {{ block.settings.color_scheme }}
                  {{ block.settings.style_border }}"
                  style="
                    padding-top:{% if product.selected_or_first_available_selling_plan_allocation or product.has_only_default_variant == false %}{{ block.settings.spacing_top }}px;{% else %}0;{% endif %}
                    padding-bottom:{% if product.selected_or_first_available_selling_plan_allocation or product.has_only_default_variant == false %}{{ block.settings.spacing_bottom }}px;{% else %}0;{% endif %}">
                  {% render 'product__options', 
                    blocksettings: block.settings,
                    is_product_template: is_product_template,
                    enable_default_selling_plan_widget: section.settings.enable_default_selling_plan_widget
                  %}
                </div>
              
              {% when 'linked_products' %}
                <div class="
                  {{ block.settings.color_border }}
                  {{ block.settings.color_scheme }}
                  {{ block.settings.style_border }}"
                  style="
                  padding-top:{{ block.settings.spacing_top }}px;
                  padding-bottom:{{ block.settings.spacing_bottom }}px;">
                  {% render 'product__linked-products', blocksettings: block.settings %}
                </div>
                
              {% when 'add' %}
                <div class="
                  {{ block.settings.color_border }}
                  {{ block.settings.color_scheme }}
                  {{ block.settings.style_border }}"
                  style="
                    padding-top:{{ block.settings.spacing_top }}px;
                    padding-bottom:{{ block.settings.spacing_bottom }}px;">
                  {% render 'product__add', 
                    blocksettings: block.settings %}
                  {% if block.settings.enable_dynamic_checkout %}
                    <div class="px-4 mt-4"
                      x-show="current_variant_exists && current_variant_available">
                      {{ form | payment_button }}
                    </div>
                  {% endif %}
                  {% unless block.settings.content == blank %}
                    <div class="w-full px-4 mt-4">
                      <div class="p-2 text-center type--small m-0--clear color__bg-overlay-1 border--radius">
                        {{ block.settings.content }}
                      </div>
                    </div>
                  {% endunless %}
                </div>
  
              {% when 'rating' %}
                {% render 'product__rating', blocksettings: block.settings %}
    
              {% when 'bundle' %}
                {% render 'product__bundle', blocksettings: block.settings %}  

              {% when 'liquid' %}
                {% render 'product__liquid', blocksettings: block.settings %}
                
              {% when 'upsell' %}
                {% render 'product__upsell', blocksettings: block.settings %}

            {% endcase %}
          {% endif %}
        {% endfor %}
      </div>

    </div>
    
    {% comment %} Details - right column {% endcomment %}
    <div 
      class="
        md:border--l-width md:border--r-width border--t-width md:border-t-0 md:sticky top-0
        {{ section.settings.color_border }}
        {{ layout_detail_size }}
      ">
      <div 
        class="z-10">

          {% comment %} Blocks {% endcomment %}
          <div>
            {% for block in section.blocks %}
              {% case block.type %}
                
                {% when 'breadcrumb' %}
                  <div class="{% unless block.settings.position == "right" %}md:hidden{% endunless %}">
                    {% render 'product__breadcrumb', blocksettings: block.settings %}
                  </div>
                  
                
                {% when 'title' %}
                  <div class="{% unless block.settings.position == "right" %}md:hidden{% endunless %}">
                    {% render 'product__title', blocksettings: block.settings %}
                  </div>

                {% when 'price' %}
                  <div class="px-4 {% unless block.settings.position == "right" %}md:hidden{% endunless %}
                      {{ block.settings.color_border }} 
                      {{ block.settings.color_scheme }} 
                      {{ block.settings.style_border }}"
                    style="
                      padding-top:{{ block.settings.spacing_top }}px;
                      padding-bottom:{{ block.settings.spacing_bottom }}px;">
                    <div class="relative flex items-end justify-between">
                      {% render 'product__price', blocksettings: block.settings %}
                    </div>
                    <div class="mt-1 type--small">
                      {{ form | payment_terms }}
                    </div>
                  </div>

                {% when 'custom' %}
                  <div class='px-4 {% unless block.settings.position == "right" %}md:hidden{% endunless %}
                    {{ block.settings.color_border }} 
                    {{ block.settings.color_scheme }} 
                    {{ block.settings.style_border }}'
                    style="
                      padding-top:{{ block.settings.spacing_top }}px;
                      padding-bottom:{{ block.settings.spacing_bottom }}px;">
                    {% render 'product__custom-field', blocksettings: block.settings %}
                  </div>
                
                {% when 'text' %}
                  <div class="{% unless block.settings.position == "right" %}md:hidden{% endunless %}">
                    {% render 'product__text', blocksettings: block.settings %}
                  </div>

                {% when 'description' %}
                  <div class="{% unless block.settings.position == "right" %}md:hidden{% endunless %}" style="
                    padding-top:{{ block.settings.spacing_top }}px;
                    padding-bottom:{{ block.settings.spacing_bottom }}px;">
                    {% render 'product__description', blocksettings: block.settings %}
                  </div>
                  

                {% when 'accordion' %}
                  <div class="{% unless block.settings.position == "right" %}md:hidden{% endunless %}">
                    {% render 'product__accordion', blocksettings: block.settings %}
                  </div>

                {% when 'popup' %}
                  <div class="{% unless block.settings.position == "right" %}md:hidden{% endunless %}">
                    {% render 'product__popup', blocksettings: block.settings %}
                  </div>

                {% when 'inventory' %}
                  <div class="{% unless block.settings.position == "right" %}md:hidden{% endunless %}">
                    {% render 'product__inventory', blocksettings: block.settings %}
                  </div>
                  
                {% when 'pickup' %}
                  <div class="{% unless block.settings.position == "right" %}md:hidden{% endunless %}">
                    {% render 'product__pickup', blocksettings: block.settings %}
                  </div>
                  
                {% when 'options' %}
                  <div class="options {% unless block.settings.position == "right" %}md:hidden{% endunless %}
                    {{ block.settings.color_border }} 
                    {{ block.settings.color_scheme }}
                    {{ block.settings.style_border }}"
                    style="
                      padding-top:{% if product.selected_or_first_available_selling_plan_allocation or product.has_only_default_variant == false %}{{ block.settings.spacing_top }}px;{% else %}0;{% endif %}
                      padding-bottom:{% if product.selected_or_first_available_selling_plan_allocation or product.has_only_default_variant == false %}{{ block.settings.spacing_bottom }}px;{% else %}0;{% endif %}">
                    {% render 'product__options', 
                      blocksettings: block.settings,
                      is_product_template: is_product_template,
                      enable_default_selling_plan_widget: section.settings.enable_default_selling_plan_widget
                    %}
                  </div>

                
                {% when 'linked_products' %}
                  <div class="{% unless block.settings.position == "right" %}md:hidden{% endunless %}
                    {{ block.settings.color_border }} 
                    {{ block.settings.color_scheme }}
                    {{ block.settings.style_border }}"
                    style="
                    padding-top:{{ block.settings.spacing_top }}px;
                    padding-bottom:{{ block.settings.spacing_bottom }}px;">
                    {% render 'product__linked-products', blocksettings: block.settings %}
                  </div>
                  
                {% when 'add' %}
                  <div class="{% unless block.settings.position == "right" %}md:hidden{% endunless %}
                    {{ block.settings.color_border }} 
                    {{ block.settings.color_scheme }}
                    {{ block.settings.style_border }}"
                    style="
                      padding-top:{{ block.settings.spacing_top }}px;
                      padding-bottom:{{ block.settings.spacing_bottom }}px;">
                    {% render 'product__add', 
                      blocksettings: block.settings %}
                    {% if block.settings.enable_dynamic_checkout %}
                      <div class="px-4 mt-4"
                        x-show="current_variant_exists && current_variant_available">
                        {{ form | payment_button }}
                      </div>
                    {% endif %}
                    {% unless block.settings.content == blank %}
                      <div class="w-full px-4 mt-4">
                        <div class="p-2 text-center type--small m-0--clear color__bg-overlay-1 border--radius">
                          {{ block.settings.content }}
                        </div>
                      </div>
                    {% endunless %}
                  </div>
    
                {% when 'rating' %}
                  <div class="{% unless block.settings.position == "right" %}md:hidden{% endunless %}">
                    {% render 'product__rating', blocksettings: block.settings %}
                  </div>
      
                {% when 'bundle' %}
                  <div class="{% unless block.settings.position == "right" %}md:hidden{% endunless %}">
                    {% render 'product__bundle', blocksettings: block.settings %}  
                  </div>

                {% when 'liquid' %}
                  <div class="{% unless block.settings.position == "right" %}md:hidden{% endunless %}">
                    {% render 'product__liquid', blocksettings: block.settings %}
                  </div>
      
                {% when 'upsell' %}
                  <div class="{% unless block.settings.position == "right" %}md:hidden{% endunless %}">
                    {% render 'product__upsell', blocksettings: block.settings %}
                  </div>

                {% when '@app' %}
                  <div class="w-full px-4 my-4">
                    {% render block %}
                  </div>
                
              {% endcase %}
            {% endfor %}
          </div>

      </div>
    </div>

    {% comment %} Sticky add to cart {% endcomment %}
    {% for block in section.blocks %}
      {% case block.type %}
        {% when 'stickyadd' %}
          {% render 'product__stickyadd', blocksettings: block.settings %}
      {% endcase %}
    {% endfor %}
    
  {% endform %}

  {% comment %} Upsell - quick adds {% endcomment %}
  {% for block in section.blocks %}
    {% case block.type %}
      {% when 'upsell' %}
        <template x-for="(item, index) in upsell_products">
          <div :id="'js-quickAdd-upsell-' + item.handle"></div>
        </template>
    {% endcase %}
  {% endfor %}
  
  {% if section.settings.enable_zoom %}
    {% render 'product__gallery-fullscreen' %}
  {% endif %}

</section>

{% render 'script__recent-view' %}

{% schema %}
