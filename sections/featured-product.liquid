<!-- sections/featured_product.liquid -->
{% comment %} 
  Featured product on any page with entire details of the product. This product can output content similar to the product page.

  Globals:
    - settings.layout_horizontal: horizontaly margin

  Possible enhancements:
    - Add setting to apply border above/below section.

  Pro tips:
    - Section will be hidden unless a valid product is selected.
{% endcomment %}

{% assign product = section.settings.product %}

{% capture script_product %}
  {% render 'script__product', 
    gallery_style: section.settings.media_gallery_style, 
    is_product_template: false, 
    featured: true, 
    enable_default_selling_plan_widget: section.settings.enable_default_selling_plan_widget
  %}
{% endcapture %}

{% case section.settings.layout_gallery_size %}
  {% when 'w-full md:w-1/3' %}
    {% assign layout_detail_size = 'w-full md:w-2/3' %}
  {% when 'w-full md:w-1/2' %}
    {% assign layout_detail_size = 'w-full md:w-1/2' %}
  {% when 'w-full md:w-2/3' %}
    {% assign layout_detail_size = 'w-full md:w-1/3' %}
  {% when 'w-full md:w-3/4' %}
    {% assign layout_detail_size = 'w-full md:w-1/4' %}
{% endcase %}

{% unless section.settings.product == null %}

  <section class="z-10 relative
    {{ section.settings.visibility }}
    {{ section.settings.color_scheme }}
    {{ section.settings.color_border }}
    {{ section.settings.style_border }}
    {% unless request.design_mode %}
      {% if settings.enable_animations %}
        opacity-50 js-animation animation-100 
      {% endif %}
    {% endunless %}"
    {% if settings.enable_animations %}
      data-delay="100" 
      data-replace="{ 
        'opacity-50' : 'opacity-100'
      }"
    {% endif %}
    style="
      padding-top:{{ section.settings.spacing_top }}px;
      padding-bottom:{{ section.settings.spacing_bottom }}px;"  
    x-data="{{ script_product }}"
    x-init="
      attachStandardForm();
      initProductForm();
    "
    x-ref="formContainer"
    @keyup.left="galleryScrollBack();"
    @keyup.right="galleryScrollNext();">

    {% capture form_class %}
      !px-0 md:!px-4 flex flex-row flex-wrap
      {{ settings.layout_horizontal }}
    {% endcapture %}
    {% form "product", product,
      class: form_class,
      x-ref: 'productForm'
    %}
  
      {% comment %} Media - left column {% endcomment %}
      <div 
        class="
          top-0 md:sticky flex flex-col
          {{ section.settings.layout_gallery_size }}
        ">
  
        {% comment %} Media {% endcomment %}
        {% if section.settings.media_gallery_style == 'grid' %}
          {% render 'product__gallery-grid', featured: true %}
        {% elsif section.settings.media_gallery_style == 'slider' %}
          {% render 'product__gallery-slider', featured: true %}
        {% else %}
          {% render 'product__gallery-thumbnail-slider', featured: true %} 
        {% endif %}
  
        {% comment %} Blocks - desktop only {% endcomment %}
        <div 
          class="hidden md:block">
          {% for block in section.blocks %}
            {% if block.settings.position == "left" %}
              {% case block.type %}
                  
                {% when 'breadcrumb' %}
                  {% render 'product__breadcrumb', blocksettings: block.settings, featured: true %}
                
                {% when 'title' %}
                  {% render 'product__title', blocksettings: block.settings, featured: true %}
  
                {% when 'price' %}
                  <div class="px-4
                      {{ block.settings.color_border }} 
                      {{ block.settings.color_scheme }} 
                      {{ block.settings.style_border }}"
                    style="
                      padding-top:{{ block.settings.spacing_top }}px;
                      padding-bottom:{{ block.settings.spacing_bottom }}px;">
                    <div class="relative flex items-end justify-between">
                      {% render 'product__price', blocksettings: block.settings, featured: true %}
                    </div>
                    <div class="mt-1 type--small">
                      {{ form | payment_terms }}
                    </div>
                  </div>
  
                {% when 'custom' %}
                  <div class='px-4
                    {{ block.settings.color_border }} 
                    {{ block.settings.color_scheme }} 
                    {{ block.settings.style_border }}'
                    style="
                      padding-top:{{ block.settings.spacing_top }}px;
                      padding-bottom:{{ block.settings.spacing_bottom }}px;">
                    {% render 'product__custom-field', blocksettings: block.settings, featured: true %}
                  </div>
                
                {% when 'text' %}
                  {% render 'product__text', blocksettings: block.settings, featured: true %}
  
                {% when 'description' %}
                  <div>
                    {% render 'product__description', blocksettings: block.settings, featured: true %}
                  </div>
  
                {% when 'accordion' %}
                  {% render 'product__accordion', blocksettings: block.settings, featured: true %}
  
                {% when 'popup' %}
                  {% render 'product__popup', blocksettings: block.settings, featured: true %}
  
                {% when 'inventory' %}
                  {% render 'product__inventory', blocksettings: block.settings, featured: true %}
                  
                {% when 'pickup' %}
                  {% render 'product__pickup', blocksettings: block.settings, featured: true %}    
                  
                {% when 'options' %}
                  <div class="options
                    {{ block.settings.color_border }}
                    {{ block.settings.color_scheme }}
                    {{ block.settings.style_border }}"
                    style="
                      padding-top:{% if product.selected_or_first_available_selling_plan_allocation or product.has_only_default_variant == false %}{{ block.settings.spacing_top }}px;{% else %}0;{% endif %}
                      padding-bottom:{% if product.selected_or_first_available_selling_plan_allocation or product.has_only_default_variant == false %}{{ block.settings.spacing_bottom }}px;{% else %}0;{% endif %}">
                    {% render 'product__options', 
                      blocksettings: block.settings,
                      is_product_template: false,
                      featured: true,
                      enable_default_selling_plan_widget: section.settings.enable_default_selling_plan_widget
                    %}
                  </div>
                
                {% when 'linked_products' %}
                  <div class="
                    {{ block.settings.color_border }}
                    {{ block.settings.color_scheme }}
                    {{ block.settings.style_border }}"
                    style="
                    padding-top:{{ block.settings.spacing_top }}px;
                    padding-bottom:{{ block.settings.spacing_bottom }}px;">
                    {% render 'product__linked-products', blocksettings: block.settings, featured: true %}
                  </div>
                  
                {% when 'add' %}
                  <div class="
                    {{ block.settings.color_border }}
                    {{ block.settings.color_scheme }}
                    {{ block.settings.style_border }}"
                    style="
                      padding-top:{{ block.settings.spacing_top }}px;
                      padding-bottom:{{ block.settings.spacing_bottom }}px;">
                    {% render 'product__add', 
                      blocksettings: block.settings, featured: true %}
                    {% if block.settings.enable_dynamic_checkout %}
                      <div class="px-4 mt-4"
                        x-show="current_variant_exists && current_variant_available">
                        {{ form | payment_button }}
                      </div>
                    {% endif %}
                    {% unless block.settings.content == blank %}
                      <div class="w-full px-4 mt-4">
                        <div class="p-2 text-center type--small m-0--clear color__bg-overlay-1 border--radius">
                          {{ block.settings.content }}
                        </div>
                      </div>
                    {% endunless %}
                  </div>
    
                {% when 'rating' %}
                  {% render 'product__rating', blocksettings: block.settings, featured: true %}
      
                {% when 'bundle' %}
                  {% render 'product__bundle', blocksettings: block.settings, featured: true %}  
  
                {% when 'liquid' %}
                  {% render 'product__liquid', blocksettings: block.settings, featured: true %}
                {% when 'upsell' %}
                  {% render 'product__upsell', blocksettings: block.settings, featured: true %}
              {% endcase %}
            {% endif %}
          {% endfor %}
        </div>
  
      </div>
      
      {% comment %} Details - right column {% endcomment %}
      <div 
        class="
          md:border--l-width md:border--r-width border--t-width md:border-t-0
          {{ section.settings.color_border }}
          {{ layout_detail_size }}
        ">
        <div 
          class="sticky top-0 z-10">
  
            {% comment %} Blocks {% endcomment %}
            <div>
              {% for block in section.blocks %}
                {% case block.type %}
                  
                  {% when 'breadcrumb' %}
                    {% if block.settings.position == "right" %}
                      {% render 'product__breadcrumb', blocksettings: block.settings, featured: true %}
                    {% endif %}
                  
                  {% when 'title' %}
                    {% if block.settings.position == "right" %}
                      {% render 'product__title', blocksettings: block.settings, featured: true %}
                    {% endif %}
  
                  {% when 'price' %}
                    {% if block.settings.position == "right" %}
                      <div class="px-4
                          {{ block.settings.color_border }} 
                          {{ block.settings.color_scheme }} 
                          {{ block.settings.style_border }}"
                        style="
                          padding-top:{{ block.settings.spacing_top }}px;
                          padding-bottom:{{ block.settings.spacing_bottom }}px;">
                        <div class="relative flex items-end justify-between">
                          {% render 'product__price', blocksettings: block.settings, featured: true %}
                        </div>
                        <div class="mt-1 type--small">
                          {{ form | payment_terms }}
                        </div>
                      </div>
                    {% endif %}
  
                  {% when 'custom' %}
                    {% if block.settings.position == "right" %}
                      <div class='px-4
                        {{ block.settings.color_border }} 
                        {{ block.settings.color_scheme }} 
                        {{ block.settings.style_border }}'
                        style="
                          padding-top:{{ block.settings.spacing_top }}px;
                          padding-bottom:{{ block.settings.spacing_bottom }}px;">
                        {% render 'product__custom-field', blocksettings: block.settings, featured: true %}
                      </div>
                    {% endif %}
                  
                  {% when 'text' %}
                    {% if block.settings.position == "right" %}
                      {% render 'product__text', blocksettings: block.settings, featured: true %}
                    {% endif %}
  
                  {% when 'description' %}
                    {% if block.settings.position == "right" %}
                      <div style="
                        padding-top:{{ block.settings.spacing_top }}px;
                        padding-bottom:{{ block.settings.spacing_bottom }}px;">
                        {% render 'product__description', blocksettings: block.settings, featured: true %}
                      </div>
                    {% endif %}
  
                  {% when 'accordion' %}
                    {% if block.settings.position == "right" %}
                      {% render 'product__accordion', blocksettings: block.settings, featured: true %}
                    {% endif %}
  
                  {% when 'popup' %}
                    {% if block.settings.position == "right" %}
                      {% render 'product__popup', blocksettings: block.settings, featured: true %}
                    {% endif %}
  
                  {% when 'inventory' %}
                    {% if block.settings.position == "right" %}
                      {% render 'product__inventory', blocksettings: block.settings, featured: true %}
                    {% endif %}
                    
                  {% when 'pickup' %}
                    {% if block.settings.position == "right" %}
                      {% render 'product__pickup', blocksettings: block.settings, featured: true %}
                    {% endif %}
                    
                  {% when 'options' %}
                    {% if block.settings.position == "right" %}
                      <div class="options
                        {{ block.settings.color_border }} 
                        {{ block.settings.color_scheme }}
                        {{ block.settings.style_border }}"
                        style="
                          padding-top:{% if product.selected_or_first_available_selling_plan_allocation or product.has_only_default_variant == false %}{{ block.settings.spacing_top }}px;{% else %}0;{% endif %}
                          padding-bottom:{% if product.selected_or_first_available_selling_plan_allocation or product.has_only_default_variant == false %}{{ block.settings.spacing_bottom }}px;{% else %}0;{% endif %}">
                        {% render 'product__options', 
                          blocksettings: block.settings,
                          is_product_template: false,
                          featured: true,
                          enable_default_selling_plan_widget: section.settings.enable_default_selling_plan_widget
                        %}
                      </div>
                    {% endif %}
                  
                  {% when 'linked_products' %}
                    {% if block.settings.position == "right" %}
                      <div class="
                        {{ block.settings.color_border }} 
                        {{ block.settings.color_scheme }}
                        {{ block.settings.style_border }}"
                        style="
                        padding-top:{{ block.settings.spacing_top }}px;
                        padding-bottom:{{ block.settings.spacing_bottom }}px;">
                        {% render 'product__linked-products', blocksettings: block.settings, featured: true %}
                      </div>
                    {% endif %}
                    
                  {% when 'add' %}
                    {% if block.settings.position == "right" %}
                      <div class="
                        {{ block.settings.color_border }} 
                        {{ block.settings.color_scheme }}
                        {{ block.settings.style_border }}"
                        style="
                          padding-top:{{ block.settings.spacing_top }}px;
                          padding-bottom:{{ block.settings.spacing_bottom }}px;">
                        {% render 'product__add', 
                          blocksettings: block.settings, featured: true %}
                        {% if block.settings.enable_dynamic_checkout %}
                          <div class="px-4 mt-4"
                            x-show="current_variant_exists && current_variant_available">
                            {{ form | payment_button }}
                          </div>
                        {% endif %}
                        {% unless block.settings.content == blank %}
                          <div class="w-full px-4 mt-4">
                            <div class="p-2 text-center type--small m-0--clear color__bg-overlay-1 border--radius">
                              {{ block.settings.content }}
                            </div>
                          </div>
                        {% endunless %}
                      </div>
                    {% endif %}
      
                  {% when 'rating' %}
                    {% if block.settings.position == "right" %}
                      {% render 'product__rating', blocksettings: block.settings, featured: true %}
                    {% endif %}
        
                  {% when 'bundle' %}
                    {% if block.settings.position == "right" %}
                      {% render 'product__bundle', blocksettings: block.settings, featured: true %}  
                    {% endif %}
  
                  {% when 'liquid' %}
                    {% if block.settings.position == "right" %}
                      {% render 'product__liquid', blocksettings: block.settings, featured: true %}
                    {% endif %}
        
                  {% when 'upsell' %}
                    {% if block.settings.position == "right" %}
                      {% render 'product__upsell', blocksettings: block.settings, featured: true %}
                    {% endif %}
  
                  {% when '@app' %}
                    <div class="w-full px-4 mt-4">
                      {% render block %}
                    </div>
                  
                {% endcase %}
              {% endfor %}
            </div>
  
        </div>
      </div>
  
      {% comment %} Sticky add to cart {% endcomment %}
      {% for block in section.blocks %}
        {% case block.type %}
          {% when 'stickyadd' %}
            {% render 'product__stickyadd', blocksettings: block.settings, featured: true %}
        {% endcase %}
      {% endfor %}
      
    {% endform %}
  
    {% comment %} Upsell - quick adds {% endcomment %}
    {% for block in section.blocks %}
      {% case block.type %}
        {% when 'upsell' %}
          <template x-for="(item, index) in upsell_products">
            <div :id="'js-quickAdd-upsell-' + item.handle"></div>
          </template>
      {% endcase %}
    {% endfor %}
    
    {% if section.settings.enable_zoom %}
      {% render 'product__gallery-fullscreen', featured: true %}
    {% endif %}
        
  </section>

{% endunless %}

{% schema %}
{
  "name":"t:sections.featured_product.name",
  "settings":[
    {
      "type":"header",
      "content":"t:sections.all.headers.header__11.content"
    },
    {
      "type":"product",
      "id":"product",
      "label":"t:sections.featured_product.name"
    },
    {
      "type":"checkbox",
      "id":"enable_default_variant",
      "label":"t:sections.product_main.settings.enable_default_variant.label",
      "info":"t:sections.product_main.settings.enable_default_variant.info",
      "default":true
    },
    {
      "type": "checkbox",
      "id": "enable_default_selling_plan_widget",
      "label": "t:settings_schema.product.settings.enable_default_selling_plan_widget.label",
      "info": "t:settings_schema.product.settings.enable_default_selling_plan_widget.info",
      "default": true
    },
    {
      "type": "header",
      "content": "t:sections.all.headers.header__22.content"
    },
    {
      "type":"range",
      "id":"spacing_top",
      "min":0,
      "max":300,
      "step":20,
      "unit":"px",
      "label":"t:sections.all.layout.settings.spacing.label_top",
      "default":0
    },
    {
      "type":"range",
      "id":"spacing_bottom",
      "min":0,
      "max":300,
      "step":20,
      "unit":"px",
      "label":"t:sections.all.layout.settings.spacing.label_bottom",
      "default":0
    },
    {
      "type":"header",
      "content":"t:sections.all.headers.header__10.content"
    },
    {
      "type":"select",
      "id":"media_gallery_style",
      "label":"t:sections.product_main.settings.media_gallery_style.label",
      "options":[
        {
          "value":"grid",
          "label":"t:sections.product_main.settings.media_gallery_style.options__1.label"
        },
        {
          "value":"slider",
          "label":"t:sections.product_main.settings.media_gallery_style.options__2.label"
        },
        {
          "value":"thumbnail-slider",
          "label":"t:sections.product_main.settings.media_gallery_style.options__3.label"
        }
      ],
      "default":"grid"
    },
    {
      "type":"select",
      "id":"image_border",
      "label":"t:sections.all.colors.settings.color_border.label",
      "options":[
        {
          "value":"border--width color__border-divider-1",
          "label":"t:sections.all.colors.settings.color_border.options__1.label"
        },
        {
          "value":"border--width color__border-selected-1",
          "label":"t:sections.all.colors.settings.color_border.options__2.label"
        },
        {
          "value":"!color__border-transparent",
          "label":"t:sections.all.colors.settings.color_border.options__3.label"
        }
      ],
      "default":"!color__border-transparent"
    },
    {
      "type":"checkbox",
      "id":"enable_zoom",
